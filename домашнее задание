#include <stdio.h>

int main() {
    int A, B, C;
    
    printf("Введите параметры кислотности A, B, C: ");
    scanf("%d %d %d", &A, &B, &C);
    
    // Проверка условия для "идеальной" лунки
    if (A % 3 == 0 && B % 3 == 0 && C % 3 == 0) {
        printf("Лунка ИДЕАЛЬНАЯ для посадки!\n");
    } else {
        printf("Лунка НЕ идеальная для посадки.\n");
    }
    
    return 0;
}
